{
  "plugins": ["simple-import-sort", "import"],
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true
  },
  "rules": {
    "quotes": "error",
    "no-unused-vars": "error",
    "sort-imports": "error"
  }
}
